    "jest-environment-node": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "nx": "16.2.2",
    "nx-cloud": "16.0.5",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.0.2"
  }
}